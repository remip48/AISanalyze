@article{taheri_improvements_2021,
	title = {Improvements in reports of species redistribution under climate change are required},
	volume = {7},
	issn = {2375-2548},
	url = {https://advances.sciencemag.org/lookup/doi/10.1126/sciadv.abe1110},
	doi = {10.1126/sciadv.abe1110},
	abstract = {Studies have documented climate change–induced shifts in species distributions but uncertainties associated with data and methods are typically unexplored. We reviewed 240 reports of climate-related species-range shifts and classified them based on three criteria. We ask whether observed distributional shifts are compared against random expectations, whether multicausal factors are examined on equal footing, and whether studies provide sufficient documentation to enable replication. We found that only {\textasciitilde}12.1\% of studies compare distributional shifts across multiple directions, {\textasciitilde}1.6\% distinguish observed patterns from random expectations, and {\textasciitilde}19.66\% examine multicausal factors. Last, {\textasciitilde}75.5\% of studies report sufficient data and results to allow replication. We show that despite gradual improvements over time, there is scope for raising standards in data and methods within reports of climate-change induced shifts in species distribution. Accurate reporting is important because policy responses depend on them. Flawed assessments can fuel criticism and divert scarce resources for biodiversity to competing priorities.},
	language = {en},
	number = {15},
	urldate = {2021-05-27},
	journal = {Science Advances},
	author = {Taheri, Shirin and Naimi, Babak and Rahbek, Carsten and Araújo, Miguel B.},
	month = apr,
	year = {2021},
	note = {Number: 15},
	pages = {eabe1110},
}

