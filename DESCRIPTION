    tidyverse (>= 2.0.0),
    dplyr (>= 1.1.2),
    sf (>= 1.0-9),
    lubridate (>= 1.9.2),
    units (>= 0.8-0),
    purrr (>= 1.0.1),
    stats (>= 4.2.1),
    utils (>= 4.2.1),
    magrittr (>= 2.0.3)
Depends: R(>= 4.2.0)
Encoding: UTF-8
